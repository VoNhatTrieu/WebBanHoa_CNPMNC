                <!-- Danh sách bình luận -->
                <div class="comments-list mt-4">
                    @Html.Action("LoadComment", "Comment", new { productId = Model.PK_iProductID })
                </div>
            </div>
        </div>
    </div>


}


@section scripts{
    <script src="js/single_custom.js"></script>
}

<script>
    document.addEventListener('DOMContentLoaded', function () {
        const minusButton = document.querySelector('.minus');
        const plusButton = document.querySelector('.plus');
        const quantityInput = document.getElementById('quantity_value');
        const addToCartButton = document.querySelector('.show-add-cart');
        const cartAlert = document.getElementById('cartAlert');

        // Hiệu ứng cộng trừ
        function animateButton(btn) {
            btn.classList.add('clicked');
            setTimeout(() => btn.classList.remove('clicked'), 200);
        }

        minusButton.addEventListener('click', () => {
            let val = parseInt(quantityInput.value, 10);
            if (val > 1) {
                quantityInput.value = val - 1;
                animateButton(minusButton);
            }
        });

        plusButton.addEventListener('click', () => {
            let val = parseInt(quantityInput.value, 10);
            quantityInput.value = val + 1;
            animateButton(plusButton);
        });

        // Gửi form thật bằng AJAX
        addToCartButton.addEventListener('click', function (e) {
            e.preventDefault();
            const form = this.closest('form');
            const formData = $(form).serialize();
            const currentQty = parseInt(quantityInput.value, 10) || 1;

            $(this).addClass('pulse');

            $.post(form.action, formData)
                .done(() => {
                    showCartAlert();
                    $(addToCartButton).removeClass('pulse');

                    // Cập nhật lại số lượng hiển thị sau khi thêm thành công
                    let cartCount = parseInt(localStorage.getItem('cartCount') || "0");
                    localStorage.setItem('cartCount', cartCount + currentQty);

                    updateCartCountDisplay(cartCount + currentQty);
                })
                .fail(() => {
                    alert('❌ Lỗi khi thêm vào giỏ hàng.');
                    $(addToCartButton).removeClass('pulse');
                });
        });

        // Hiển thị thông báo
        function showCartAlert() {
            cartAlert.classList.add('show');
            setTimeout(() => cartAlert.classList.remove('show'), 2500);
        }

        // Cập nhật số lượng hiển thị trên giao diện (nếu có giỏ hàng ở header)
        function updateCartCountDisplay(count) {
            const badge = document.getElementById('cart-count');
            if (badge) {
                badge.textContent = count;
            }
        }

        // Khi tải lại trang, lấy lại giá trị lưu trong localStorage
        const savedCount = parseInt(localStorage.getItem('cartCount') || "0");
        updateCartCountDisplay(savedCount);
    });
    // JavaScript cho phần bình luận
document.addEventListener('DOMContentLoaded', function () {
    // Khởi tạo biến
    const commentForm = document.querySelector('.comment-form');
    const commentsList = document.querySelector('.comments-list');
    const commentInput = document.querySelector('textarea[name="content"]');

    // Nếu có form bình luận (đã đăng nhập)
    if (commentForm) {
        commentForm.addEventListener('submit', function (e) {
            e.preventDefault();

            if (!commentInput.value.trim()) {
                alert('Vui lòng nhập nội dung bình luận!');
                return;
            }

            // Gửi bình luận bằng AJAX
            $.ajax({
                url: '@Url.Action("AddComment", "Comment")',
                type: 'POST',
                data: {
                    productId: @Model.PK_iProductID,
                    content: commentInput.value,
                    __RequestVerificationToken: $('input[name="__RequestVerificationToken"]').val()
                },
                success: function () {
                    // Làm mới danh sách bình luận
                    $.get('@Url.Action("LoadComment", "Comment")', { productId: @Model.PK_iProductID }, function (data) {
                        commentsList.innerHTML = data;
                        commentInput.value = ''; // Xóa nội dung đã nhập
                        showCommentAlert('✅ Đã gửi bình luận thành công!');
                    });
                },
                error: function () {
                    alert('❌ Có lỗi xảy ra khi gửi bình luận.');
                }
            });
        });
    }

    // Hiển thị thông báo sau khi bình luận
    function showCommentAlert(message) {
        const alert = document.createElement('div');
        alert.className = 'comment-alert';
        alert.textContent = message;
        document.body.appendChild(alert);

        setTimeout(() => {
            alert.classList.add('show');
            setTimeout(() => {
                alert.classList.remove('show');
                setTimeout(() => alert.remove(), 300);
            }, 2000);
        }, 100);
    }
});

</script>




















































